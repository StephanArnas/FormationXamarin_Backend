ccess\",\r\n            \"content\": {\r\n              \"application/json\": {\r\n                \"schema\": {\r\n                  \"$ref\": \"#/components/schemas/ProfileWrapped\"\r\n                }\r\n              }\r\n            }\r\n          }\r\n        }\r\n      }\r\n    },\r\n    \"/v1/Password/Send\": {\r\n      \"post\": {\r\n        \"tags\": [\r\n          \"Password\"\r\n        ],\r\n        \"summary\": \"Send an email with a link to reset the password.\",\r\n        \"requestBody\": {\r\n          \"description\": \"Contains the user email.\",\r\n          \"content\": {\r\n            \"application/json-patch+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/EmailValueApi\"\r\n              }\r\n            },\r\n            \"application/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/EmailValueApi\"\r\n              }\r\n            },\r\n            \"text/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/EmailValueApi\"\r\n              }\r\n            },\r\n            \"application/*+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/EmailValueApi\"\r\n              }\r\n            }\r\n          }\r\n        },\r\n        \"responses\": {\r\n          \"204\": {\r\n            \"description\": \"Success\"\r\n          }\r\n        }\r\n      }\r\n    },\r\n    \"/v1/Password/Reset\": {\r\n      \"post\": {\r\n        \"tags\": [\r\n          \"Password\"\r\n        ],\r\n        \"summary\": \"Reset the user password.\",\r\n        \"requestBody\": {\r\n          \"description\": \"Contains the new password and the token to allow the operation.\",\r\n          \"content\": {\r\n            \"application/json-patch+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/TokenPasswordApi\"\r\n              }\r\n            },\r\n            \"application/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/TokenPasswordApi\"\r\n              }\r\n            },\r\n            \"text/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/TokenPasswordApi\"\r\n              }\r\n            },\r\n            \"application/*+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/TokenPasswordApi\"\r\n              }\r\n            }\r\n          }\r\n        },\r\n        \"responses\": {\r\n          \"204\": {\r\n            \"description\": \"Success\"\r\n          }\r\n        }\r\n      }\r\n    },\r\n    \"/v1/Profile/Get/{profileId}\": {\r\n      \"get\": {\r\n        \"tags\": [\r\n          \"Profile\"\r\n        ],\r\n        \"summary\": \"Get a profile.\",\r\n        \"parameters\": [\r\n          {\r\n            \"name\": \"profileId\",\r\n            \"in\": \"path\",\r\n            \"description\": \"Profile ID.\",\r\n            \"required\": true,\r\n            \"schema\": {\r\n              \"type\": \"integer\",\r\n              \"format\": \"int32\"\r\n            }\r\n          }\r\n        ],\r\n        \"responses\": {\r\n          \"200\": {\r\n            \"description\": \"Success\",\r\n            \"content\": {\r\n              \"application/json\": {\r\n                \"schema\": {\r\n                  \"$ref\": \"#/components/schemas/ProfileApi\"\r\n                }\r\n              }\r\n            }\r\n          }\r\n        }\r\n      }\r\n    },\r\n    \"/v1/Profile/Create\": {\r\n      \"post\": {\r\n        \"tags\": [\r\n          \"Profile\"\r\n        ],\r\n        \"summary\": \"Create a new profile.\",\r\n        \"responses\": {\r\n          \"201\": {\r\n            \"description\": \"Success\",\r\n            \"content\": {\r\n              \"application/json\": {\r\n                \"schema\": {\r\n                  \"$ref\": \"#/components/schemas/ProfileApi\"\r\n                }\r\n              }\r\n            }\r\n          }\r\n        }\r\n      }\r\n    },\r\n    \"/v1/Profile/Update/{profileId}\": {\r\n      \"put\": {\r\n        \"tags\": [\r\n          \"Profile\"\r\n        ],\r\n        \"summary\": \"Update a profile.\",\r\n        \"parameters\": [\r\n          {\r\n            \"name\": \"profileId\",\r\n            \"in\": \"path\",\r\n            \"description\": \"Profile ID.\",\r\n            \"required\": true,\r\n            \"schema\": {\r\n              \"type\": \"integer\",\r\n              \"format\": \"int32\"\r\n            }\r\n          }\r\n        ],\r\n        \"requestBody\": {\r\n          \"description\": \"Profile.\",\r\n          \"content\": {\r\n            \"application/json-patch+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/ProfileApi\"\r\n              }\r\n            },\r\n            \"application/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/ProfileApi\"\r\n              }\r\n            },\r\n            \"text/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/ProfileApi\"\r\n              }\r\n            },\r\n            \"application/*+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/ProfileApi\"\r\n              }\r\n            }\r\n          }\r\n        },\r\n        \"responses\": {\r\n          \"204\": {\r\n            \"description\": \"Success\"\r\n          }\r\n        }\r\n      }\r\n    },\r\n    \"/v1/Profile/UpdateName/{profileId}\": {\r\n      \"put\": {\r\n        \"tags\": [\r\n          \"Profile\"\r\n        ],\r\n        \"summary\": \"Update a profile.\",\r\n        \"parameters\": [\r\n          {\r\n            \"name\": \"profileId\",\r\n            \"in\": \"path\",\r\n            \"description\": \"Profile ID.\",\r\n            \"required\": true,\r\n            \"schema\": {\r\n              \"type\": \"integer\",\r\n              \"format\": \"int32\"\r\n            }\r\n          }\r\n        ],\r\n        \"requestBody\": {\r\n          \"description\": \"Profile.\",\r\n          \"content\": {\r\n            \"application/json-patch+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/ProfileNameApi\"\r\n              }\r\n            },\r\n            \"application/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/ProfileNameApi\"\r\n              }\r\n            },\r\n            \"text/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/ProfileNameApi\"\r\n              }\r\n            },\r\n            \"application/*+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/ProfileNameApi\"\r\n              }\r\n            }\r\n          }\r\n        },\r\n        \"responses\": {\r\n          \"204\": {\r\n            \"description\": \"Success\"\r\n          }\r\n        }\r\n      }\r\n    },\r\n    \"/v1/Profile/Remove/{profileId}\": {\r\n      \"delete\": {\r\n        \"tags\": [\r\n          \"Profile\"\r\n        ],\r\n        \"summary\": \"Delete a profile.\",\r\n        \"parameters\": [\r\n          {\r\n            \"name\": \"profileId\",\r\n            \"in\": \"path\",\r\n            \"description\": \"Profile ID.\",\r\n            \"required\": true,\r\n            \"schema\": {\r\n              \"type\": \"integer\",\r\n              \"format\": \"int32\"\r\n            }\r\n          }\r\n        ],\r\n        \"responses\": {\r\n          \"204\": {\r\n            \"description\": \"Success\"\r\n          }\r\n        }\r\n      }\r\n    },\r\n    \"/v1/Profile/Import\": {\r\n      \"post\": {\r\n        \"tags\": [\r\n          \"Profile\"\r\n        ],\r\n        \"summary\": \"Create a new SingleProfile 