      "json": "{\r\n  \"openapi\": \"3.0.1\",\r\n  \"info\": {\r\n    \"title\": \"SingleProfile API\",\r\n    \"description\": \"Backend for SingleProfile project.\",\r\n    \"contact\": {\r\n      \"name\": \"Stephan Arnas\",\r\n      \"url\": \"https://www.linkedin.com/in/stephan-arnas/\",\r\n      \"email\": \"stephan.arnas@gmail.com\"\r\n    },\r\n    \"version\": \"v1\"\r\n  },\r\n  \"servers\": [\r\n    {\r\n      \"url\": \"/api\"\r\n    }\r\n  ],\r\n  \"paths\": {\r\n    \"/v1/Auth/Login\": {\r\n      \"post\": {\r\n        \"tags\": [\r\n          \"Auth\"\r\n        ],\r\n        \"summary\": \"Log in into SingleProfile with a login / password.\",\r\n        \"description\": \"Sample request:\\r\\n            \\r\\n    POST /Login\\r\\n    {\\r\\n       \\\"email\\\": \\\"stephan.arnas@gmail.com\\\",\\r\\n       \\\"password\\\": \\\"Aze123\\\"\\r\\n    }\",\r\n        \"requestBody\": {\r\n          \"description\": \"Credential information.\",\r\n          \"content\": {\r\n            \"application/json-patch+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/LoginApi\"\r\n              }\r\n            },\r\n            \"application/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/LoginApi\"\r\n              }\r\n            },\r\n            \"text/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/LoginApi\"\r\n              }\r\n            },\r\n            \"application/*+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/LoginApi\"\r\n              }\r\n            }\r\n          }\r\n        },\r\n        \"responses\": {\r\n          \"200\": {\r\n            \"description\": \"Success\",\r\n            \"content\": {\r\n              \"application/json\": {\r\n                \"schema\": {\r\n                  \"$ref\": \"#/components/schemas/TokenApi\"\r\n                }\